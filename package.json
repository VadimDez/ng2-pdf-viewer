{
  "name": "ng2-pdf-viewer",
  "version": "0.0.3",
  "description": "Angular2 component for rendering PDF",
  "main": "dist/pdf-viewer.component.js",
  "typings": "./dist/pdf-viewer.component.min.d.ts",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings install",
    "all": "concurrently \"tsc -w \" \"tsc -p runtime -w \" \"npm run lite\" ",
    "watch": "tsc -w ",
    "lite": "lite-server",
    "minify": "uglifyjs ./dist/pdf-viewer.component.js --compress --mangle --output ./dist/pdf-viewer.component.min.js --source-map ./dist/pdf-viewer.component.min.js.map",
    "clean": "gulp clean",
    "compile": "gulp compile",
    "build": "gulp build",
    "start": "concurrently --kill-others \"gulp watch\" \"lite-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://vadimdez@github.com/VadimDez/ng2-pdf-viewer.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "pdf"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VadimDez/ng2-pdf-viewer/issues"
  },
  "homepage": "https://github.com/VadimDez/ng2-pdf-viewer#readme",
  "peerDependencies": {
    "@angular/core": "2.0.0-rc.2",
    "pdfjs-dist": "^1.5.296"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "es6-shim": "^0.35.0",
    "pdfjs-dist": "^1.5.296",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.31",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.25",
    "concurrently": "^2.1.0",
    "del": "^2.2.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^2.0.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^6.0.1",
    "gulp-typescript": "^2.12.0",
    "lite-server": "^2.2.0",
    "moment": "^2.14.1",
    "systemjs-builder": "^0.15.23",
    "ts-node": "^0.9.3",
    "tslint": "^3.6.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "uglifyjs": "^2.4.10"
  },
  "files": [
    "dist"
  ]
}
