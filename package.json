{
  "name": "ng2-pdf-viewer",
  "version": "7.0.1",
  "description": "Angular 5+ component for rendering PDF",
  "license": "MIT",
  "scripts": {
    "// - Binaries": "Run against node_modules/.bin",
    "ng": "ng",
    "start-test": "start-server-and-test",
    "// - Building": "Build library and app",
    "build": "ng build --prod",
    "build:stats": "ng build --stats-json",
    "build:ssr": "ng build --prod && ng run ng2-pdf-viewer:server:production",
    "build:library": "ng-packagr -p package.json",
    "// - Serve": "Serve apps in different modes",
    "start": "ng serve",
    "serve:ssr": "node dist-integration-server/main",
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "test": "ng test --watch false",
    "test:w": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "ng build --prod --output-path docs --base-href ng2-pdf-viewer && cp ./docs/index.html ./docs/404.html",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "// - Cypress": "Cypress scripts",
    "cy:open": "cypress open",
    "cy:open:ssr": "cypress open --config integrationFolder=cypress/integration-ssr",
    "cy:run": "cypress run",
    "cy:run:ssr": "cypress run --config integrationFolder=cypress/integration-ssr",
    "// - CI": "CI Testing",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "e2e:ci:ssr": "start-test serve:ssr 4200 cy:run:ssr"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://vadimdez@github.com/VadimDez/ng2-pdf-viewer.git"
  },
  "keywords": [
    "angular",
    "pdf",
    "angular",
    "angular-pdf",
    "angular2-pdf",
    "ng2",
    "ng2-pdf",
    "angular5"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "bugs": {
    "url": "https://github.com/VadimDez/ng2-pdf-viewer/issues"
  },
  "homepage": "https://github.com/VadimDez/ng2-pdf-viewer#readme",
  "dependencies": {
    "pdfjs-dist": "~2.7.570",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "pdfjs-dist": "~2.7.570"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/animations": "^11.1.1",
    "@angular/cdk": "^8.2.1",
    "@angular/cli": "~11.1.2",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@angular/material": "^8.2.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/platform-server": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@nguniversal/builders": "~11.1.1",
    "@nguniversal/express-engine": "~11.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.7.2",
    "codelyzer": "^6.0.0",
    "cypress": "^7.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "rxjs": "~6.5.3",
    "start-server-and-test": "^1.12.2",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "typescript": "~4.1.3",
    "webpack-bundle-analyzer": "^3.7.0",
    "zone.js": "~0.10.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "allowedNonPeerDependencies": [
      "pdfjs-dist"
    ]
  }
}
